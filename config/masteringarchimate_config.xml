<configuration archimateVersion='3.0'>
    <elementLabel context="node" includeGrouping='true' includeType='false' includeStereotype='true'/>
    <elementLabel context="element" includeGrouping='true' includeType='true'/>
    <elementLabel context="relationship" includeGrouping='true' includeType='false'/>
    
    <mappings>
        <elementmapping type="ApplicationComponent" alias="Application Component"/>
        <elementmapping type="ApplicationFunction" alias="Application Function"/>
        <elementmapping type="SystemSoftware" alias="System Software"/>
        <elementmapping type="BusinessActor" alias="Business Actor"/>
        <elementmapping type="BusinessRole" alias="Business Role"/>
        <elementmapping type="BusinessEvent" alias="Business Event"/>
        
        <relationshipmapping sourceType="any" targetType="any" type="AssociationRelationship"
            direction="to" alias="relates to"/>
        <relationshipmapping sourceType="any" targetType="any" type="AssociationRelationship"
            direction="from" alias="relates to"/>
        <relationshipmapping sourceType="any" targetType="any" type="AssignmentRelationship"
            direction="to" alias="performs"/>
        <relationshipmapping sourceType="any" targetType="any" type="AssignmentRelationship"
            direction="from" alias="is performed by"/>
        <relationshipmapping sourceType="any" targetType="any" type="FlowRelationship"
            direction="to" alias="flows to"/>
        <relationshipmapping sourceType="any" targetType="any" type="FlowRelationship"
            direction="from" alias="flows from"/>
        <relationshipmapping sourceType="any" targetType="any" type="UsedByRelationship"
            direction="to" alias="used by"/>
        <relationshipmapping sourceType="any" targetType="any" type="UsedByRelationship"
            direction="from" alias="uses"/>
        <relationshipmapping sourceType="any" targetType="any" type="CompositionRelationship"
            direction="from" alias="is part of"/>
        <relationshipmapping sourceType="any" targetType="any" type="CompositionRelationship"
            direction="to" alias="contains"/>
        <relationshipmapping sourceType="any" targetType="any" type="AggregationRelationship"
            direction="from" alias="is part of"/>
        <relationshipmapping sourceType="any" targetType="any" type="AggregationRelationship"
            direction="to" alias="contains"/>
        <relationshipmapping sourceType="any" targetType="any" type="RealisationRelationship"
            direction="from" alias="is realised by"/>
        <relationshipmapping sourceType="any" targetType="any" type="RealisationRelationship"
            direction="to" alias="realises"/>
        <relationshipmapping sourceType="any" targetType="any" type="AccessRelationship"
            direction="from" alias="is accessed by"/>
        <relationshipmapping sourceType="any" targetType="any" type="AccessRelationship"
            direction="to" alias="accesses"/>
        <relationshipmapping sourceType="any" targetType="any" type="ServingRelationship"
            direction="to" alias="services"/>
        <relationshipmapping sourceType="any" targetType="any" type="ServingRelationship"
            direction="from" alias="is serviced by"/>
        <!-- specific mappings -->
        <relationshipmapping sourceType="Node" targetType="Artifact" type="AssignmentRelationship"
            direction="from" alias="resides on"/>
    </mappings>
